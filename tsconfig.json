{
  "compilerOptions": {
    "outDir": "./dist/",
    "noImplicitAny": true,
    "module": "es6",
    "target": "es5",
    "jsx": "react",
    "allowJs": true,
    "moduleResolution": "node",
    "noImplicitAny": false,
    "downlevelIteration": true
  },
  "include": ["./src/**/*.tsx", "images.d.ts"],
  "exclude": ["dist", "build", "node_modules"]
}